    <validationRules>
        <fullName>Credit_Card_Number_format</fullName>
        <active>true</active>
        <errorConditionFormula>And( NOT( ISBLANK( Credit_Card_Number__c ) ) ,
NOT(REGEX(Credit_Card_Number__c ,&quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;)))</errorConditionFormula>
        <errorMessage>Credit_Card_Number should be formatted in 9999-9999-9999-9999 or 9999999999999999 number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Weeked_validation</fullName>
        <active>true</active>
        <errorConditionFormula>CASE(MOD(  date__c  - DATE(1900, 1, 7), 7), 0, 0, 6, 0, 1)=0</errorConditionFormula>
        <errorMessage>date should  weekday</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_of_five_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (MOD( Multiple_Of_Five__c ,5)!=0) , True, false)</errorConditionFormula>
        <errorMessage>Enter a value multiple of five</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Neumeric_Account_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  NOT(ISBLANK( AccountNumber)),
  NOT ( ISNUMBER( AccountNumber ))
)</errorConditionFormula>
        <errorMessage>Account Number Should numeric if not blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Mix_Pct__c  &lt;0.0 ,Mix_Pct__c  &gt;1.0 )</errorConditionFormula>
        <errorMessage>Enter a value  between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postal_Code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 OR(BillingCountry =&quot;USA&quot;,BillingCountry =&quot;US&quot;) ,
  NOT(REGEX( BillingPostalCode ,&quot;\\d{5}(-\\d{4})?&quot;))   
)</errorConditionFormula>
        <errorMessage>Account Billing Zip/Postal Code Should be  in 99999 or 99999-9999 format if Billing Country is USA or US</errorMessage>
    </validationRules>
