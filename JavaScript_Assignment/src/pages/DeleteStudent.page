<apex:page standardController="Student__c" extensions="DeleteStudentController" recordSetVar="Student">
    <apex:form >
        <apex:pageBlock >
        <apex:pageBlockTable value="{!studentList}" var="M">
            <apex:column value="{!M.Name}"/>
            
        </apex:pageBlockTable>
        <apex:pageBlockButtons >
            <apex:commandButton value="Continue" action="{!deleteStudents}"/>
            <apex:commandButton value="Cancel" action="{!getReferer}"/>
        </apex:pageBlockButtons>
    </apex:pageBlock>
    </apex:form>
    
</apex:page>